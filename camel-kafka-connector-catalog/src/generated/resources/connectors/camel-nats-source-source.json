{
	"connector": {
		"class": "org.apache.camel.kafkaconnector.natssource.CamelNatssourceSourceConnector",
		"artifactId": "camel-nats-source-kafka-connector",
		"groupId": "org.apache.camel.kafkaconnector",
		"id": "camel-nats-source-source",
		"type": "source",
		"version": "0.11.0-SNAPSHOT",
		"description": "Receive data from NATS topics."
	},
	"properties": {
		"camel.kamelet.nats-source.topic": {
			"name": "camel.kamelet.nats-source.topic",
			"description": "NATS Topic name",
			"priority": "HIGH",
			"required": "true"
		},
		"camel.kamelet.nats-source.servers": {
			"name": "camel.kamelet.nats-source.servers",
			"description": "Comma separated list of NATS Servers",
			"priority": "HIGH",
			"required": "true"
		}
	}
}
